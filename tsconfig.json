{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES5",
    "lib": ["es2015", "dom"],
    "typeRoots": ["node_modules/@types", "typings"]
  },
  "exclude": ["node_modules", "typings/browser.d.ts", "typings/browser"]
}
